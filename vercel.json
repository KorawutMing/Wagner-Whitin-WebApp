{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.9"
        }
      }
    ],
    "env": {
      "PIP_NO_CACHE_DIR": "true"
    },
    "buildCommand": "pip install --upgrade pip && pip install --upgrade -r requirements.txt"
  }
  